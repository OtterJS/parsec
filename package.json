{
  "name": "body-parsec",
  "version": "2.0.5",
  "repository": "https://github.com/talentlessguy/parsec.git",
  "author": "talentlessguy <pilll.PL22@gmail.com>",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.esm.js",
  "engines": {
    "node": ">=13"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./koa": {
      "import": "./dist/koa.js",
      "require": "./dist/cjs/koa.js"
    },
    "./package.json": "./package.json",
    "./": "./"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/jest": "^26.0.0",
    "@types/koa": "^2.11.3",
    "@types/node": "^14.0.12",
    "@types/supertest": "^2.0.9",
    "express": "^4.17.1",
    "jest": "^26.0.1",
    "koa": "^2.12.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.0",
    "tslib": "^2.0.0",
    "typescript": "^3.9.5"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "test": "jest",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "build:esm": "tsc --target esnext --outDir dist",
    "build": "pnpm run build:cjs && pnpm run build:esm",
    "prepare": "pnpm run build && pnpm run test"
  },
  "peerDependencies": {
    "koa": "^2.12.0"
  }
}
